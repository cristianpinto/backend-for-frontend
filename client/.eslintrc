{
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "block-scoped-var": 2,
    "camelcase": 1,
    "comma-dangle": 2,
    "comma-spacing": 2,
    "curly": 1,
    "dot-notation": 1,
    "eol-last": 2,
    "eqeqeq": [2, "allow-null"],
    "indent": [2, 2],
    "jsx-quotes": [1, "prefer-double"],
    "new-cap": 1,
    "no-debugger": 1,
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-spaces": 2,
    "no-nested-ternary": 1,
    "no-trailing-spaces": 1,
    "no-unreachable": 2,
    "no-unused-vars": 2,
    "no-var": 2,
    "quotes": [2, "single"],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
    "semi": [2, "always"],
    "strict": [2, "never"],
    "use-isnan": 2,
    "yoda": 1
  },
  "plugins": [
    "react"
  ]
}
